{
  "name": "mario-portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "build-client": "npm run build",
    "server": "nodemon server.js --ignore client",
