    "loaders.css": "^0.1.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.22",
    "normalize.css": "^8.0.1",
    "openai": "^3.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-leaflet": "^4.2.1",
    "react-loaders": "^3.0.1",
    "react-router-dom": "^6.14.0",
    "sass": "^1.63.6",
